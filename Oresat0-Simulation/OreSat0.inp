F     INTEGER NLOADT,NVER,NID,NUM_NODES, NUM_REG, LOCAL_NODE, LOCAL_COUNTER
F     CALL USRFIL(NLOADT,FILE,'OLD')
F     WRITE(NOUT,*) 'Loading temperatures from ', file
F     READ(NLOADT,101,ERR=996)NVER
F     READ(NLOADT,102,ERR=997)NUM_NODES
F     DO LOCAL_COUNTER=1,NUM_NODES
F     READ(NLOADT,103,ERR=998)SUB,NID,A__TD
F     LOCAL_NODE = INTNOD(SUB,-NID)
